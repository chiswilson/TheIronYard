<div class="n1">
	<a class="newpost" href="/newpost">Create New Post...</a>

</div>


<div class="listings">
        <div class="row">
          <div class="col-sm-12">
            <ul class="list-group">

            	<% @blogposts.each do |post| %>

            	  <div class="l0">
            	  	<div>
            	  		<h5>&nbsp;</h5>
            	  	</div>

            	    <div class="dib">
            	        <h6>Posted by: </h6>
            	    </div>

            	    <div class="l2">
            	   	    <h6><%= post[:userid] %> </h6>
            	    </div>

            	    <div class="dib">
            	    	<h6>&nbsp; &nbsp; <%= post[:timestamp]%> &nbsp; &nbsp;</h6>

            	    </div>

            	    <div class="l4">
            	    	<!-- <a class="click-me" href="/index">Clike me to navigate</a> -->

            	    	<button type="button" class="btn btn-xs btn-default listingbutton">Edit</button>
            	    	
            	    	<button type="button" class="btn btn-xs btn-default listingbutton">Delete</button>

            	    </div>
            	               	    
            	  <li class="list-group-item"> <%= post[:msg]%> </li>  
            	  </div>    
            	     
            	<% end %> 
              
            </ul>
          </div><!-- /.col-sm-12 -->
        </div>  

</div>