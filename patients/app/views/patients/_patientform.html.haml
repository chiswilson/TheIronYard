/ %h2 testing

= form_for [@hospital, @patient] do |f|
	= f.label :name, "Name:"
	= f.text_field :name
	%br
	= f.label :dateofbirth, "Date of Birth:"
	= f.text_field :dateofbirth
	%br
	= f.submit

	= link_to "Cancel", hospital_path(@hospital)

